#the log size(MB) per process. Max value is 2000 MB
nvm.size = 1024
chunk.size = 4096
copy.strategy = 1
debug.enable = 0
nvram.wbw = 19
pfile.location = /dev/shm
#rmt.chkpt.enable = 0
#rmt.rstart.enable = 0
##this is a restart run or not - 1 or 0
rstart = 0 
##offset used during buddy selection
buddy.offset = 1 
##if not -1, we do a naive split
split.ratio = 5
##TRADITIONAL = 0 , ONLINE = 1
cr.type = 1
##assigned free memory per MPI node in MB
free.memory = 1 
#enables the early copy
early.copy.enabled = 1
##time increment if invalidation
early.copy.offset = 150000
##helper cpu cores
helper.cores = 14 
helper.cores = 15
